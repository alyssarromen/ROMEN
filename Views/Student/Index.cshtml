@{
}

<h1>Student List</h1>

<table class="table table-dark">
    <tr>
        <td>Name:</td>
        <td>@ViewBag.StudentModel.StudentName</td>
    </tr>
    <tr>
        <td>Student ID:</td>
        <td>@ViewBag.StudentModel.StudentId</td>
    </tr>
    <tr>
        <td>Date Enrolled:</td>
        <td>@ViewBag.StudentModel.DateEnrolled</td>
    </tr>
    <tr>
        <td>Student Course:</td>
        <td>@ViewBag.StudentModel.StudentCourse</td>
    </tr>
    <tr>
        <td>Email:</td>
        <td>@ViewBag.StudentModel.Email</td>
    </tr>
</table>